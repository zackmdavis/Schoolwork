\end_inset

 Near a zero 
\begin_inset Formula $z_{j}$
\end_inset

 of order 
\begin_inset Formula $m$
\end_inset

, 
\begin_inset Formula $f(z)=(z-z_{j})^{m}h(z)$
\end_inset

 for some holomorphic 
\begin_inset Formula $h(z)$
\end_inset

 nonzero in the neighborhood of 
\begin_inset Formula $z_{j}$
\end_inset

.
 Then, differentiating, 
\begin_inset Formula $f'(z)=m(z-z_{j})^{m-1}h(z)+(z-z_{j})^{m}h'(z)$
\end_inset

.
 Then, dividing by 
\begin_inset Formula $f$
\end_inset

, 
\begin_inset Formula $\frac{f'(z)}{f(z)}=\frac{m(z-z_{0})^{m-1}h(z)+(z-z_{j})^{m}h'(z)}{(z-z_{j})^{m}h(z)}=\frac{m(z-z_{j})^{\cancel{m}-1}\cancel{h(z)}}{\cancel{(z-z_{j})^{m}}\cancel{h(z)}}+\frac{\cancel{(z-z_{j})^{m}}h'(z)}{\cancel{(z-z_{j})^{m}}h(z)}=\frac{m}{z-z_{j}}+\frac{h'(z)}{h(z)}$
\end_inset

.
 And multiplying by 
\begin_inset Formula $g(z)$
\end_inset

, we get 
\begin_inset Formula $\frac{f'(z)}{f(z)}g(z)=\frac{mg(z)}{z-z_{j}}+\frac{h'(z)}{h(z)}$
\end_inset

.
 
\end_layout

\begin_layout Standard
Similarly, near a pole 
\begin_inset Formula $p_{j}$
\end_inset

 of order 
\begin_inset Formula $n$
\end_inset

, 
\begin_inset Formula $f(z)=(z-p_{j})^{-n}h(z)$
\end_inset

 for some nonzero 
\begin_inset Formula $h(z)$
\end_inset

, it's the same thing: 
\begin_inset Formula $f'(z)=-n(z-p_{j})^{-n-1}h(z)+(z-p_{j})^{-n}h'(z)$
\end_inset

, so 
\begin_inset Formula $\frac{f'(z)}{f(z)}=\frac{-n(z-p_{j})^{-n-1}h(z)+(z-p_{j})^{-n}h'(z)}{(z-p_{j})^{-n}h(z)}=\frac{-n(z-p_{j})^{\cancel{-n}-1}\cancel{h(z)}}{\cancel{(z-p_{j})^{-n}}\cancel{h(z)}}+\frac{\cancel{(z-p_{j})^{-n}}h'(z)}{\cancel{(z-p_{j})^{-n}}h(z)}=\frac{-n}{z-p_{j}}+\frac{h'(z)}{h(z)}$
\end_inset

, so 
\begin_inset Formula $\text{\frac{f'(z)}{f(z)}}g(z)=\frac{-ng(z)}{z-p_{j}}+\frac{h'(z)}{h(z)}$
\end_inset

.
 
\end_layout

\begin_layout Standard
Then the residues go like:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\text{Res}(\frac{f'(z)}{f(z)}g(z),\ z_{j})=2\pi i\lim_{z\rightarrow z_{j}}\cancel{(z-z_{j})}\frac{mg(z)}{\cancel{z-z_{j}}}+\cancelto{0}{(z-z_{j})\frac{h'(z)}{h(z)}}=mg(z)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\text{Res}(\frac{f'(z)}{f(z)}g(z),\ p_{j})=2\pi i\lim_{z\rightarrow p_{j}}\cancel{(z-p_{j})}\frac{-ng(z)}{\cancel{z-p_{j}}}+\cancelto{0}{(z-p_{j})\frac{h'(z)}{h(z)}}=ng(z)
\]

\end_inset


\end_layout

